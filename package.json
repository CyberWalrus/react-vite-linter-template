{
    "name": "react-vite-linter-template",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "start": "vite",
        "build": "tsc && vite build --base=./",
        "preview": "vite preview",
        "ts:check": "tsc --noEmit",
        "eslint:check": "eslint --max-warnings 0 --ext .jsx,.js,.ts,.tsx src/",
        "eslint:fix": "eslint --fix --ext .jsx,.js,.ts,.tsx src/",
        "stylelint:check": "stylelint \"src/**/*.scss\"",
        "stylelint:fix": "stylelint \"src/**/*.scss\" --fix",
        "lint:check": "yarn eslint:check && yarn stylelint:check && yarn ts:check",
        "lint:fix": "yarn eslint:fix && yarn stylelint:fix",
        "test": "TZ=Europe/Moscow vitest run --isolate=false --threads=false"
    },
    "dependencies": {
        "clsx": "1.2.1",
        "react": "18.2.0",
        "react-dom": "18.2.0"
    },
    "devDependencies": {
        "@greenly/stylelint-config-rational-order": "0.2.1",
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "14.0.0",
        "@testing-library/user-event": "14.4.3",
        "@types/mocha": "10.0.1",
        "@types/node": "18.15.11",
        "@types/react": "18.0.34",
        "@types/react-dom": "18.0.11",
        "@typescript-eslint/eslint-plugin": "5.58.0",
        "@typescript-eslint/parser": "5.58.0",
        "@vitejs/plugin-react-swc": "3.3.0",
        "@vitest/coverage-c8": "0.30.1",
        "@vitest/ui": "0.30.1",
        "eslint": "8.38.0",
        "eslint-config-airbnb": "19.0.4",
        "eslint-config-airbnb-typescript": "17.0.0",
        "eslint-config-airbnb-typescript-prettier": "5.0.0",
        "eslint-config-prettier": "8.8.0",
        "eslint-config-react-app": "7.0.1",
        "eslint-import-resolver-alias": "1.1.2",
        "eslint-import-resolver-typescript": "3.5.5",
        "eslint-plugin-filenames": "1.3.2",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-jsx-a11y": "6.7.1",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-react": "7.32.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-simple-import-sort": "10.0.0",
        "eslint-plugin-sort-keys-fix": "1.1.2",
        "eslint-plugin-typescript-sort-keys": "2.3.0",
        "postcss": "8.4.21",
        "postcss-scss": "4.0.6",
        "prettier": "2.8.7",
        "sass": "1.61.0",
        "stylelint": "15.4.0",
        "stylelint-config-css-modules": "4.2.0",
        "stylelint-config-recommended": "11.0.0",
        "stylelint-config-standard": "32.0.0",
        "stylelint-csstree-validator": "2.1.0",
        "stylelint-declaration-block-no-ignored-properties": "2.7.0",
        "stylelint-no-browser-hacks": "1.2.1",
        "stylelint-order": "6.0.3",
        "stylelint-scss": "4.6.0",
        "stylelint-z-index-value-constraint": "1.3.0",
        "typescript": "5.0.4",
        "typescript-plugin-css-modules": "5.0.1",
        "vite": "4.2.1",
        "vitest": "0.30.1"
    },
    "packageManager": "yarn@3.6.1"
}
