{
    "name": "react-vite-linter-template",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "start": "concurrently \"yarn dev\" \"yarn run ts:scss:watch\"",
        "dev": "vite",
        "build": "tsc && vite build",
        "build:visualizer": "yarn vite-bundle-visualizer",
        "preview": "vite preview",
        "ts:scss:build": "yarn typed-scss-modules src",
        "ts:scss:watch": "yarn typed-scss-modules src --outputFolder __generated__ --watch",
        "ts:check": "tsc --noEmit",
        "eslint:check": "eslint --max-warnings 0 --ext .jsx,.js,.ts,.tsx src/",
        "eslint:fix": "eslint --fix --ext .jsx,.js,.ts,.tsx src/",
        "stylelint:check": "stylelint \"src/**/*.scss\"",
        "stylelint:fix": "stylelint \"src/**/*.scss\" --fix",
        "lint:check": "yarn eslint:check && yarn stylelint:check && yarn ts:check",
        "lint:fix": "yarn eslint:fix && yarn stylelint:fix",
        "test": "TZ=Europe/Moscow vitest run --isolate=false",
        "test:server": "VITEST=true vite",
        "test:update": "UPDATE_SCREENSHOTS=true TZ=Europe/Moscow vitest run --isolate=false"
    },
    "dependencies": {
        "@radix-ui/themes": "2.0.3",
        "@tanstack/react-router": "1.16.6",
        "@tanstack/router-devtools": "1.16.6",
        "clsx": "2.1.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "zustand": "4.5.1"
    },
    "devDependencies": {
        "@cyber-walrus/configs": "1.1.1",
        "@testing-library/jest-dom": "6.4.2",
        "@testing-library/react": "14.2.1",
        "@types/pixelmatch": "5.2.6",
        "@types/pngjs": "6.0.4",
        "@types/react": "18.2.58",
        "@types/react-dom": "18.2.19",
        "@vitejs/plugin-react-swc": "3.6.0",
        "concurrently": "8.2.2",
        "jsdom": "24.0.0",
        "pixelmatch": "5.3.0",
        "playwright": "1.41.2",
        "pngjs": "7.0.0",
        "tsc-alias": "1.8.8",
        "typed-scss-modules": "8.0.0",
        "vite": "5.1.4",
        "vite-bundle-visualizer": "1.0.1",
        "vite-plugin-pwa": "0.19.0",
        "vite-plugin-static-copy": "1.0.1",
        "vitest": "1.3.1"
    }
}